<h1>Expenses</h1>
<table id="expense_lists" class="display" cellspacing="0" width="100%">
  <thead>
    <tr>
      <th>Date</th>
      <th>Payer</th>
      <th>Description</th>
       <th>Amount</th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <td><%= @expense.date %></td>
        <td><%= @expense.payer %></td>
        <td><%= @expense.description %></td>
        <td><%= @expense.amount %></td>
      </tr>
  </tbody>
</table>
